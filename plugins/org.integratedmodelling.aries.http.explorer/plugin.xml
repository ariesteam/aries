<?xml version="1.0" ?>
<!DOCTYPE plugin PUBLIC "-//JPF//Java Plug-in Manifest 1.0" "http://jpf.sourceforge.net/plugin_1_0.dtd">
<plugin id="org.integratedmodelling.aries.http.explorer" version="0.8.0" class="org.integratedmodelling.aries.webapp.ARIESWebappPlugin">
	
	<requires>
		<import plugin-id="org.integratedmodelling.thinklab.core" reverse-lookup="true"/>
		<import plugin-id="org.integratedmodelling.thinklab.searchengine" reverse-lookup="true"/>
		<import plugin-id="org.integratedmodelling.thinklab.modelling" reverse-lookup="true"/>
		<import plugin-id="org.integratedmodelling.thinklab.mca" reverse-lookup="true"/>
		<import plugin-id="org.integratedmodelling.aries.aries" />
		<import plugin-id="org.integratedmodelling.thinklab.http"/>
		<import plugin-id="org.integratedmodelling.thinklab.http.geospace"/>
	</requires>	
	
	<runtime>
		<library id="plugin" path="classes/" type="code"> 
			<export prefix="*"/>
		</library> 
		<library type="resources" path="/" id="web" />
		<library type="resources" path="config/" id="config" />
	</runtime>

</plugin>
