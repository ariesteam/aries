  <!-- protected status from national WDPA database, used as a presence/absence coverage -->
  <measurement:Ranking id="WPA2a">
    <observation:hasObservable>
      <conservation:ProtectedStatus />
    </observation:hasObservable>
    <rdfs:comment>World Database on Protected Areas</rdfs:comment>
    <metadata:hasPriority>10</metadata:hasPriority>
    <metadata:hasURL>http://www.wdpa.org/</metadata:hasURL>
    <observation:hasDataSource>
      <geospace:WFSDataSource>
        <geospace:hasServiceUrl>http://ecoinformatics.uvm.edu/geoserver/wfs</geospace:hasServiceUrl>
        <geospace:hasCoverageId>global:wdpa2a</geospace:hasCoverageId>
      </geospace:WFSDataSource>
    </observation:hasDataSource>
    <observation:hasObservationExtent>
      <geospace:ArealFeatureSet>
        <geospace:hasCoordinateReferenceSystem>EPSG:4326</geospace:hasCoordinateReferenceSystem>
        <geospace:hasLatLowerBound>-68.65255775673205</geospace:hasLatLowerBound>
        <geospace:hasLonLowerBound>-179.99999963965692</geospace:hasLonLowerBound>
        <geospace:hasLatUpperBound>83.7299995730599</geospace:hasLatUpperBound>
        <geospace:hasLonUpperBound>179.9995396992789</geospace:hasLonUpperBound>
      </geospace:ArealFeatureSet>
    </observation:hasObservationExtent>
  </measurement:Ranking>

  <!-- Native units are in gallons/minute, as a point file. To convert
       this to aerial units, we assume each well to be 4 m^2 in aerial
       extent (2x2 m). We convert gallons to cubic meters, divide
       cubic meters by 4 m^2, convert m to mm, and convert minutes to
       year. This gives an overall conversion factor of 497,349 -->
  <measurement:Measurement id="well_tested_rate">
    <observation:hasObservable>
      <waterSupplyService:AnnualWellCapacity />
    </observation:hasObservable>
    <rdfs:comment>Data source: Arizona Department of Water Resources Wells-55 database</rdfs:comment>
    <metadata:hasPriority>0</metadata:hasPriority>
    <metadata:hasURL>https://gisweb.azwater.gov/WellRegistry/MapHelp.aspx</metadata:hasURL>
    <measurement:unit>mm</measurement:unit>'
    <observation:hasDataSource>
      <geospace:WCSDataSource>
        <geospace:hasServiceUrl>http://ecoinformatics.uvm.edu/geoserver/wcs</geospace:hasServiceUrl>
        <geospace:hasCoverageId>san_pedro:well_tested_rate_san_pedro</geospace:hasCoverageId>
        <geospace:hasTransformation>self * 497349</geospace:hasTransformation>
        <geospace:hasNodataValue>-3.4028234663852886E38</geospace:hasNodataValue>
      </geospace:WCSDataSource>
    </observation:hasDataSource>
    <observation:hasObservationExtent>
      <geospace:RasterGrid>
        <geospace:hasXRangeMax>7234</geospace:hasXRangeMax>
        <geospace:hasYRangeMax>8413</geospace:hasYRangeMax>
        <geospace:hasLatLowerBound>3415000.0</geospace:hasLatLowerBound>
        <geospace:hasLonLowerBound>142225.0</geospace:hasLonLowerBound>
        <geospace:hasLatUpperBound>4097784.5</geospace:hasLatUpperBound>
        <geospace:hasLonUpperBound>684925.0</geospace:hasLonUpperBound>
        <geospace:hasCoordinateReferenceSystem>EPSG:26912</geospace:hasCoordinateReferenceSystem>
      </geospace:RasterGrid>
    </observation:hasObservationExtent>
  </measurement:Measurement>

