        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&eserv;hasUsage"/>
                <owl:someValuesFrom rdf:resource="#RenewableHuntingFishingGathering"/>
            </owl:Restriction>
        </rdfs:subClassOf>
