            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&eserv;hasProvision"/>
                <owl:someValuesFrom rdf:resource="#ScenicLandscapes"/>
