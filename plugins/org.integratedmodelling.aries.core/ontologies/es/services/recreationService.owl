            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&eserv;hasUsage"/>
                <owl:someValuesFrom rdf:resource="#NonrenewableHuntingFishingGathering"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#isBeneficialFor"/>
                <owl:someValuesFrom rdf:resource="#Recreationists"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&eserv;hasProvision"/>
