            <owl:Restriction>
                <owl:onProperty rdf:resource="&eserv;ecologicalFocus"/>
                <owl:someValuesFrom rdf:resource="#UniqueLandscapeFeatures"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
