<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY DOLCE-Lite "http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#" >
    <!ENTITY protege "http://protege.stanford.edu/plugins/owl/protege#" >
    <!ENTITY eserv "http://www.integratedmodelling.org/ks/es/eserv.owl#" >
    <!ENTITY nutrientService "http://www.integratedmodelling.org/ks/es/nutrientService.owl#" >
    <!ENTITY ecology "http://www.integratedmodelling.org/ks/ecology/eco/ecology.owl#" >
    <!ENTITY habitat "http://www.integratedmodelling.org/ks/ecology/eco/habitat.owl#" >
    <!ENTITY soilretention "http://www.integratedmodelling.org/ks/es/soilRetentionService#" >
    <!ENTITY soilRetentionService "http://www.integratedmodelling.org/ks/es/soilRetentionService.owl#" >
]>


<rdf:RDF xmlns="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#"
     xml:base="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl"
     xmlns:soilRetentionService="http://www.integratedmodelling.org/ks/es/soilRetentionService.owl#"
     xmlns:DOLCE-Lite="http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#"
     xmlns:ecology="http://www.integratedmodelling.org/ks/ecology/eco/ecology.owl#"
     xmlns:nutrientService="http://www.integratedmodelling.org/ks/es/nutrientService.owl#"
     xmlns:eserv="http://www.integratedmodelling.org/ks/es/eserv.owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:protege="http://protege.stanford.edu/plugins/owl/protege#"
     xmlns:habitat="http://www.integratedmodelling.org/ks/ecology/eco/habitat.owl#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:soilretention="http://www.integratedmodelling.org/ks/es/soilRetentionService#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
    <owl:Ontology rdf:about="http://www.integratedmodelling.org/ks/es/nutrientService.owl">
        <owl:versionIRI rdf:resource="http://www.integratedmodelling.org/ks/es/nutrientService.owl"/>
        <owl:imports rdf:resource="http://www.integratedmodelling.org/ks/es/soilRetentionService"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <owl:AnnotationProperty rdf:about="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#leadsToChanged">
        <rdfs:comment rdf:datatype="&xsd;string">Climate change may have uncertain effects.</rdfs:comment>
        <rdfs:domain rdf:resource="&soilretention;ClimateChange"/>
    </owl:AnnotationProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#ObjectProperty_26 -->

    <owl:ObjectProperty rdf:about="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#ObjectProperty_26">
        <rdfs:domain rdf:resource="&soilretention;IncreasedStormRainfall"/>
    </owl:ObjectProperty>
    


    <!-- http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#leadsToChanged -->

    <owl:ObjectProperty rdf:about="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#leadsToChanged">
        <rdfs:comment rdf:datatype="&xsd;string">Climate change may have uncertain effects.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.integratedmodelling.org/ks/es/soilRetentionService#leadsToMore -->

    <owl:ObjectProperty rdf:about="&soilretention;leadsToMore"/>
    


    <!-- http://www.integratedmodelling.org/ks/es/soilRetentionService#matters-in-event-of -->

    <owl:ObjectProperty rdf:about="&soilretention;matters-in-event-of"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#AgriculturalFields -->

    <owl:Class rdf:about="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#AgriculturalFields">
        <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#NutrientReleasingHabitat"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&DOLCE-Lite;has-quality"/>
                <owl:someValuesFrom rdf:resource="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#NAgriculturalRelease"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&DOLCE-Lite;has-quality"/>
                <owl:someValuesFrom rdf:resource="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#PAgriculturalRelease"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="&xsd;string">Intensive agriculture can lead to both soil erosion (producing sediment runoff) and nutrients in runoff from excess fertilizer.  Weller et al. (1996) has P loading coefficients for agricultural lands in rural watersheds for the basin.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#AquaticNPool -->

    <owl:Class rdf:about="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#AquaticNPool">
        <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#NitrogenPools"/>
        <rdfs:comment rdf:datatype="&xsd;string">Nitrate enters aquatic systems through runoff.  There it can be taken up by algae, plankton, and other aquatic organisms.  Denitrification returns nitrogen to the atmosphere.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#AquaticPPool -->

    <owl:Class rdf:about="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#AquaticPPool">
        <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#PhosphorusPools"/>
        <rdfs:comment rdf:datatype="&xsd;string">Phosphate enters aquatic systems through runoff.  There it can be taken up by algae, plankton, and other aquatic organisms.  Sedimentation eventually can return phosphorus to the sediment and rock pool.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#AquaticTrophicStatus -->

    <owl:Class rdf:about="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#AquaticTrophicStatus">
        <rdfs:subClassOf rdf:resource="&habitat;PollutionQuality"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&DOLCE-Lite;generically-dependent-on"/>
                <owl:someValuesFrom rdf:resource="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#NutrientLimitingFactor"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="&xsd;string">The nutrient content and productivity level of an aquatic system.  Although systems can be naturally oligotrophic, mesotrophic, or eutrophic, excessive anthropogenic nutrient inputs are acting as pollutants that increase the productivity of aquatic ecosystems.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#AtmosphericNPool -->

    <owl:Class rdf:about="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#AtmosphericNPool">
        <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#NitrogenPools"/>
        <rdfs:comment rdf:datatype="&xsd;string">The atmosphere is a major pool of molecular nitrogen (N2), which composes 78% of the atmosphere.  Atmospheric nitrogen becomes biologically available through nitrogen fixation.  Denitrification returns nitrate to the atmosphere.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#BioticNPool -->

    <owl:Class rdf:about="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#BioticNPool">
        <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#NitrogenPools"/>
        <rdfs:comment rdf:datatype="&xsd;string">Nitrogen is incorporated into biological molecules in plants or animals.  Nitrogen fixing bacteria convert atmospheric nitrogen into biologically useful nitrogen.  This can exist in three forms - ammonia, nitrite, and nitrate.  Nitrate is generally the more biologically-useful form; it can be taken up by plants (then animals) or run off into aquatic systems.  Denitrification returns nitrogen to the atmosphere.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#BioticPPool -->

    <owl:Class rdf:about="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#BioticPPool">
        <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#PhosphorusPools"/>
        <rdfs:comment rdf:datatype="&xsd;string">Phosphorus is incorporated into biological molecules in plants or animals.  Dissolved phosphorus from weathering of rocks can be taken up by plants, then consumed by animals.  Excretion and death and decomposition return phosphorus to the soil and water.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#Denitrification -->

    <owl:Class rdf:about="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#Denitrification">
        <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#NitrogenCycling"/>
        <rdfs:comment rdf:datatype="&xsd;string">The return of nitrate (NO3) in organic matter, soil, or water to atmospheric nitrogen (N2) by bacteria.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#Eutrophic -->

    <owl:Class rdf:about="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#Eutrophic">
        <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#AquaticTrophicStatus"/>
        <rdfs:comment rdf:datatype="&xsd;string">An aqutatic system with high productivity and nutrient content.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#ExposedSoil -->

    <owl:Class rdf:about="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#ExposedSoil">
        <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#UrbanAreas"/>
        <rdfs:comment rdf:datatype="&xsd;string">Areas where soil is disturbed for long periods of time (e.g., construction sites) may contribute excess sediment to the watershed.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#Forest -->

    <owl:Class rdf:about="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#Forest">
        <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#NutrientRetainingHabitat"/>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#Forests -->

    <owl:Class rdf:about="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#Forests">
        <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#NutrientReleasingHabitat"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&DOLCE-Lite;has-quality"/>
                <owl:someValuesFrom rdf:resource="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#NBioticRelease"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&DOLCE-Lite;has-quality"/>
                <owl:someValuesFrom rdf:resource="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#PBioticRelease"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="&xsd;string">Forests may naturally export some nutrients.  Weller et al. (1996) has P loading coefficients for forests in rural watersheds for the basin.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#Freezing -->

    <owl:Class rdf:about="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#Freezing">
        <rdfs:subClassOf rdf:resource="&habitat;SoilQuality"/>
        <rdfs:comment rdf:datatype="&xsd;string">Frozen soils allow for essentially zero infiltration.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#HumanNApplication -->

    <owl:Class rdf:about="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#HumanNApplication">
        <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#NitrogenCycling"/>
        <rdfs:comment rdf:datatype="&xsd;string">Humans apply large amounts of nitrogen as fertilizers, especially to agricultural fields and suburban lawns (at least in the U.S.).</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#HumanPApplication -->

    <owl:Class rdf:about="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#HumanPApplication">
        <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#PhosphorusCycling"/>
        <rdfs:comment rdf:datatype="&xsd;string">Humans apply large amounts of phosphorus as fertilizers, especially to agricultural fields and suburban lawns (at least in the U.S.).</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#ImperviousSurface -->

    <owl:Class rdf:about="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#ImperviousSurface">
        <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#UrbanAreas"/>
        <rdfs:comment rdf:datatype="&xsd;string">Impervious surfaces increase runoff rates, increasing the likelihood that nutrients will run off into aquatic systems.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#InfiltrationCapacity -->

    <owl:Class rdf:about="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#InfiltrationCapacity">
        <rdfs:subClassOf rdf:resource="&habitat;SoilQuality"/>
        <rdfs:comment rdf:datatype="&xsd;string">Different soil types have different infiltration capacities.  See http://www.ems-i.com/wmshelp/Hydrologic_Models/Calculators/Composite_Runoff_Coefficients/Selecting_the_Method.htm</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#Lawns -->

    <owl:Class rdf:about="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#Lawns">
        <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#UrbanAreas"/>
        <rdfs:comment rdf:datatype="&xsd;string">At least in the U.S., many lawns are fertilized with nitrogen and phosphorus to keep them green and growing quickly.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#Mesotrophic -->

    <owl:Class rdf:about="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#Mesotrophic">
        <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#AquaticTrophicStatus"/>
        <rdfs:comment rdf:datatype="&xsd;string">An aqutatic system with intermediate productivity and nutrient content.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#NAgriculturalRelease -->

    <owl:Class rdf:about="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#NAgriculturalRelease">
        <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#HumanNApplication"/>
        <rdfs:comment rdf:datatype="&xsd;string">In the Lake Champlain Basin, agricultural area declined 5.4% from 1992-2001.  Agriculture accounted for 39% of P input, with 0.61 kg P/ha-yr released into the watershed (Troy et al. 2007).</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#NBioticRelease -->

    <owl:Class rdf:about="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#NBioticRelease">
        <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#NitrogenCycling"/>
        <rdfs:comment rdf:datatype="&xsd;string">Through excreation or death and decomposition of organisms, biotic nitrogen is re-released into ammonia (NH4), which is eventually converted into nitrite (NO2) and nitrate (NO3).</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#NBioticUptake -->

    <owl:Class rdf:about="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#NBioticUptake">
        <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#NitrogenCycling"/>
        <rdfs:comment rdf:datatype="&xsd;string">Uptake of nitrogen by an organism, typically terrestrial or aquatic plants or plankton.  When these organisms are consumed, nitrogen moves through the food web.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#NExposedSoils -->

    <owl:Class rdf:about="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#NExposedSoils">
        <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#NUrbanRelease"/>
        <rdfs:comment rdf:datatype="&xsd;string">Construction sites and eroding streambanks are sediment sources, which can then bind nutrients and lead to their runoff into aquatic systems.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#NFertilizer -->

    <owl:Class rdf:about="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#NFertilizer">
        <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#NAgriculturalRelease"/>
        <rdfs:comment rdf:datatype="&xsd;string">Direct application of fertilizer to fields often leads to its runoff.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#NFixation -->

    <owl:Class rdf:about="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#NFixation">
        <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#NitrogenCycling"/>
        <rdfs:comment rdf:datatype="&xsd;string">Fixation of nitrogen by bacteria moves N2 from the atmosphere to the soil, making it biologically available.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#NLawnFertilizer -->

    <owl:Class rdf:about="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#NLawnFertilizer">
        <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#NUrbanRelease"/>
        <rdfs:comment rdf:datatype="&xsd;string">Lawn fertilizer is a major nutrient source in urban areas.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#NManure -->

    <owl:Class rdf:about="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#NManure">
        <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#NAgriculturalRelease"/>
        <rdfs:comment rdf:datatype="&xsd;string">Closely related to the number and density of animals on the farm, and how their waste is managed.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#NPetWaste -->

    <owl:Class rdf:about="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#NPetWaste">
        <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#NUrbanRelease"/>
        <rdfs:comment rdf:datatype="&xsd;string">Pet waste can run directly into sewers or water bodies.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#NPointRelease -->

    <owl:Class rdf:about="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#NPointRelease">
        <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#HumanNApplication"/>
        <rdfs:comment rdf:datatype="&xsd;string">Point source release of nutrients; in the U.S., point source discharges have declined in recent decades.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#NRunoff -->

    <owl:Class rdf:about="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#NRunoff">
        <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#NitrogenCycling"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&DOLCE-Lite;generically-dependent-on"/>
                <owl:someValuesFrom rdf:resource="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#ImperviousSurface"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&DOLCE-Lite;generically-dependent-on"/>
                <owl:someValuesFrom rdf:resource="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#ExposedSoil"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="&xsd;string">Movement of nitrate (NO3), typically from the soil to aquatic systems.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#NSepticRelease -->

    <owl:Class rdf:about="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#NSepticRelease">
        <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#NUrbanRelease"/>
        <rdfs:comment rdf:datatype="&xsd;string">Release of nutrients from old, improperly maintained, damaged, or leaking septic systems (Moore et al. 2003).</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#NUrbanRelease -->

    <owl:Class rdf:about="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#NUrbanRelease">
        <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#HumanNApplication"/>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#NitrogenCycling -->

    <owl:Class rdf:about="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#NitrogenCycling">
        <rdfs:subClassOf rdf:resource="&ecology;NutrientFlow"/>
        <rdfs:comment rdf:datatype="&xsd;string">The processes that move nitrogen between its different chemical forms and pools.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#NitrogenPools -->

    <owl:Class rdf:about="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#NitrogenPools">
        <rdfs:subClassOf rdf:resource="&ecology;MaterialStock"/>
        <rdfs:comment rdf:datatype="&xsd;string">Nitrogen naturally cycles between the atmosphere, soil, water, and living things.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#NutrientAssimilatingEcosystem -->

    <owl:Class rdf:about="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#NutrientAssimilatingEcosystem">
        <rdfs:subClassOf rdf:resource="&eserv;EcosystemProducer"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&DOLCE-Lite;has-quality"/>
                <owl:someValuesFrom rdf:resource="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#NutrientAssimilationCapacity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
