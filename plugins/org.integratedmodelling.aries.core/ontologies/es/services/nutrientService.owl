    <owl:Class rdf:about="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#PhosphorusPools">
        <rdfs:subClassOf rdf:resource="&ecology;MaterialStock"/>
        <rdfs:comment rdf:datatype="&xsd;string">Phosphorus naturally cycles between the sediments and rocks, soil, water, and living things.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#PresenceOfOtherElements -->

    <owl:Class rdf:about="http://www.integratedmodelling.org/ks/es/nutrientEcology.owl#PresenceOfOtherElements">
