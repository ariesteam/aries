<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY owl2xml "http://www.w3.org/2006/12/owl2-xml#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY DOLCE-Lite "http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#" >
    <!ENTITY protege "http://protege.stanford.edu/plugins/owl/protege#" >
    <!ENTITY eserv "http://www.integratedmodelling.org/ks/es/eserv.owl#" >
    <!ENTITY carbonEcology "http://www.integratedmodelling.org/ks/es/carbonEcology.owl#" >
    <!ENTITY ecology "http://www.integratedmodelling.org/ks/ecology/eco/ecology.owl#" >
    <!ENTITY habitat "http://www.integratedmodelling.org/ks/ecology/eco/habitat.owl#" >
]>


<rdf:RDF xmlns="http://www.integratedmodelling.org/ks/es/carbonEcology.owl#"
     xml:base="http://www.integratedmodelling.org/ks/es/carbonEcology.owl"
     xmlns:owl2xml="http://www.w3.org/2006/12/owl2-xml#"
     xmlns:DOLCE-Lite="http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#"
     xmlns:ecology="http://www.integratedmodelling.org/ks/ecology/eco/ecology.owl#"
     xmlns:eserv="http://www.integratedmodelling.org/ks/es/eserv.owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:carbonEcology="http://www.integratedmodelling.org/ks/es/carbonEcology.owl#"
     xmlns:protege="http://protege.stanford.edu/plugins/owl/protege#"
     xmlns:habitat="http://www.integratedmodelling.org/ks/ecology/eco/habitat.owl#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#">
    <owl:Ontology rdf:about="">
        <owl:imports rdf:resource="http://www.integratedmodelling.org/ks/ecology/eco/habitat.owl"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.integratedmodelling.org/ks/ecology/eco/ecology.owl#dependsOnProcess -->

    <owl:ObjectProperty rdf:about="&ecology;dependsOnProcess"/>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#UndergoesProcess -->

    <owl:ObjectProperty rdf:about="#UndergoesProcess">
        <rdfs:comment rdf:datatype="&xsd;string"></rdfs:comment>
        <rdfs:domain rdf:resource="#CStoringOrSequesteringHabitat"/>
    </owl:ObjectProperty>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#leadsToLess -->

    <owl:ObjectProperty rdf:about="#leadsToLess">
        <rdfs:comment rdf:datatype="&xsd;string"
            >Climate change leads to less of this certain condition</rdfs:comment>
        <rdfs:domain rdf:resource="#ClimateChange"/>
    </owl:ObjectProperty>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#leadsToMore -->

    <owl:ObjectProperty rdf:about="#leadsToMore">
        <rdfs:comment rdf:datatype="&xsd;string"
            >Climate change leads to more of this certain condition</rdfs:comment>
        <rdfs:domain rdf:resource="#ClimateChange"/>
    </owl:ObjectProperty>
    


    <!-- http://www.integratedmodelling.org/ks/es/eserv.owl#hasProvision -->

    <owl:ObjectProperty rdf:about="&eserv;hasProvision"/>
    


    <!-- http://www.integratedmodelling.org/ks/es/eserv.owl#hasUsage -->

    <owl:ObjectProperty rdf:about="&eserv;hasUsage"/>
    


    <!-- http://www.integratedmodelling.org/ks/es/eserv.owl#producesBenefit -->

    <owl:ObjectProperty rdf:about="&eserv;producesBenefit"/>
    


    <!-- http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#generic-dependent -->

    <owl:ObjectProperty rdf:about="&DOLCE-Lite;generic-dependent"/>
    


    <!-- http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#generically-dependent-on -->

    <owl:ObjectProperty rdf:about="&DOLCE-Lite;generically-dependent-on"/>
    


    <!-- http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#has-quality -->

    <owl:ObjectProperty rdf:about="&DOLCE-Lite;has-quality"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.integratedmodelling.org/ks/ecology/eco/ecology.owl#DirectedInteraction -->

    <owl:Class rdf:about="&ecology;DirectedInteraction"/>
    


    <!-- http://www.integratedmodelling.org/ks/ecology/eco/ecology.owl#MaterialEnergyFlow -->

    <owl:Class rdf:about="&ecology;MaterialEnergyFlow">
        <rdfs:comment rdf:datatype="&xsd;string"></rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/ecology/eco/ecology.owl#MaterialFlow -->

    <owl:Class rdf:about="&ecology;MaterialFlow"/>
    


    <!-- http://www.integratedmodelling.org/ks/ecology/eco/ecology.owl#MaterialStock -->

    <owl:Class rdf:about="&ecology;MaterialStock"/>
    


    <!-- http://www.integratedmodelling.org/ks/ecology/eco/habitat.owl#ClimateQuality -->

    <owl:Class rdf:about="&habitat;ClimateQuality"/>
    


    <!-- http://www.integratedmodelling.org/ks/ecology/eco/habitat.owl#EcosystemQuality -->

    <owl:Class rdf:about="&habitat;EcosystemQuality"/>
    


    <!-- http://www.integratedmodelling.org/ks/ecology/eco/habitat.owl#FireRegime -->

    <owl:Class rdf:about="&habitat;FireRegime"/>
    


    <!-- http://www.integratedmodelling.org/ks/ecology/eco/habitat.owl#Habitat -->

    <owl:Class rdf:about="&habitat;Habitat"/>
    


    <!-- http://www.integratedmodelling.org/ks/ecology/eco/habitat.owl#HabitatMatrix -->

    <owl:Class rdf:about="&habitat;HabitatMatrix">
        <rdfs:comment rdf:datatype="&xsd;string"
            >A habitat made up of one or more habitats.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/ecology/eco/habitat.owl#HabitatQuality -->

    <owl:Class rdf:about="&habitat;HabitatQuality"/>
    


    <!-- http://www.integratedmodelling.org/ks/ecology/eco/habitat.owl#Herbaceous -->

    <owl:Class rdf:about="&habitat;Herbaceous">
        <rdfs:comment rdf:datatype="&xsd;string"></rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/ecology/eco/habitat.owl#HumanPopulation -->

    <owl:Class rdf:about="&habitat;HumanPopulation"/>
    


    <!-- http://www.integratedmodelling.org/ks/ecology/eco/habitat.owl#ManagementStructure -->

    <owl:Class rdf:about="&habitat;ManagementStructure"/>
    


    <!-- http://www.integratedmodelling.org/ks/ecology/eco/habitat.owl#PrimaryProductivity -->

    <owl:Class rdf:about="&habitat;PrimaryProductivity">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&DOLCE-Lite;generically-dependent-on"/>
                <owl:someValuesFrom rdf:resource="#Rainfall"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&DOLCE-Lite;generically-dependent-on"/>
                <owl:someValuesFrom rdf:resource="#MeanGrowingSeasonTemp"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&DOLCE-Lite;generically-dependent-on"/>
                <owl:someValuesFrom rdf:resource="#GrowingSeasonLength"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="&xsd;string"
            >NPP is critical to carbon sequestration rates.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/ecology/eco/habitat.owl#ShrubLand -->

    <owl:Class rdf:about="&habitat;ShrubLand">
        <rdfs:comment rdf:datatype="&xsd;string"></rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/ecology/eco/habitat.owl#Slope -->

    <owl:Class rdf:about="&habitat;Slope">
        <rdfs:comment rdf:datatype="&xsd;string"
            >Slope affects runoff speed and length of time that infiltration is possible.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/ecology/eco/habitat.owl#SoilQuality -->

    <owl:Class rdf:about="&habitat;SoilQuality"/>
    


    <!-- http://www.integratedmodelling.org/ks/ecology/eco/habitat.owl#TreeLand -->

    <owl:Class rdf:about="&habitat;TreeLand">
        <rdfs:comment rdf:datatype="&xsd;string"></rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/ecology/eco/habitat.owl#VegetationType -->

    <owl:Class rdf:about="&habitat;VegetationType">
        <rdfs:comment rdf:datatype="&xsd;string"
            >Different vegetation types will have different rooting depths, root densities, and transpiration rates, affecting their ability to aid in infiltration of water and/or slowing of wind and wave energy.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#Afforestation -->

    <owl:Class rdf:about="#Afforestation">
        <rdfs:subClassOf rdf:resource="#CarbonCyclingLandUseChanges"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Replanting of trees by humans, with the goal of increasing forest cover.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#AgriculturalPractices -->

    <owl:Class rdf:about="#AgriculturalPractices">
        <rdfs:subClassOf rdf:resource="&habitat;ManagementStructure"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >A given set of agricultural practices bring soil carbon sequestration and storage rates into equilibrium.  Changing these practices can alter both the uptake/release rate and the soil C saturation level, as it reaches a new equilibrium (Ogle et al. 2005, West and Six 2007)</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#Agriculture -->

    <owl:Class rdf:about="#Agriculture">
        <rdfs:subClassOf rdf:resource="#CStoringOrSequesteringHabitat"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Areas primarily used by humans for raising food.  The storage of carbon, particularly in their soils, is an important process.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#AnthropogenicGeologicSequestration -->

    <owl:Class rdf:about="#AnthropogenicGeologicSequestration">
        <rdfs:subClassOf rdf:resource="#GeologicSequestration"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >The intentional sequestration of CO2 into geologic formations to store it in the lithosphere rather than the atmosphere.  This is generally an energy-intensive process.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#AnthropogenicRelease -->

    <owl:Class rdf:about="#AnthropogenicRelease">
        <rdfs:subClassOf rdf:resource="#CarbonCyclingProcesses"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Extracting and burning fossil fuels and biomass converts terrestrial carbon, including that formerly stored in the earth, to atmospheric CO2.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#AtmosphereCPool -->

    <owl:Class rdf:about="#AtmosphereCPool">
        <rdfs:subClassOf rdf:resource="&ecology;MaterialStock"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&ecology;dependsOnProcess"/>
                <owl:someValuesFrom rdf:resource="#CarbonCyclingProcesses"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Atmospheric carbon, primarily as CO2.  Atmospheric CO2 concentrations are currently around 380 ppm; 450 ppm is currently recognized as a potentially dangerous threshold for climate change effects.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#BiomassBurning -->

    <owl:Class rdf:about="#BiomassBurning">
        <rdfs:subClassOf rdf:resource="#AnthropogenicRelease"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Burning of biomass moves carbon from the biosphere to the atmosphere.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#BiosphereCPool -->

    <owl:Class rdf:about="#BiosphereCPool">
        <rdfs:subClassOf rdf:resource="&ecology;MaterialStock"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Carbon currently tied up in terrestrial or oceanic systems.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#CSequestration -->

    <owl:Class rdf:about="#CSequestration">
        <rdfs:subClassOf rdf:resource="&habitat;EcosystemQuality"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&ecology;dependsOnProcess"/>
                <owl:someValuesFrom rdf:resource="#Photosynthesis"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&DOLCE-Lite;generically-dependent-on"/>
                <owl:someValuesFrom rdf:resource="&habitat;PrimaryProductivity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&DOLCE-Lite;generically-dependent-on"/>
                <owl:someValuesFrom rdf:resource="#VegetativeCover"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&DOLCE-Lite;generically-dependent-on"/>
                <owl:someValuesFrom rdf:resource="#VegetationType"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Ability of an ecosystem to sequester more carbon through photosynthesis than it releases through respiration and decomposition (and GHG equivalents for methanogenesis).</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#CStorage -->

    <owl:Class rdf:about="#CStorage">
        <rdfs:subClassOf rdf:resource="&habitat;EcosystemQuality"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Ability of an ecosystem to store carbon relatively permanently (on human time scales, and in the absence of disturbance) in soils, peat, or standing biomass.  Storage capacity in Madagascar ranges from 160-400 tons/ha, depending on rainfall (Rarivoarivelomanana 2001).</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#CStoringOrSequesteringHabitat -->

    <owl:Class rdf:about="#CStoringOrSequesteringHabitat">
        <rdfs:subClassOf rdf:resource="&habitat;Habitat"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#UndergoesProcess"/>
                <owl:someValuesFrom rdf:resource="#Photosynthesis"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#UndergoesProcess"/>
                <owl:someValuesFrom rdf:resource="#Respiration"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#UndergoesProcess"/>
                <owl:someValuesFrom rdf:resource="#Decomposition"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&DOLCE-Lite;has-quality"/>
                <owl:someValuesFrom rdf:resource="#CSequestration"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&DOLCE-Lite;has-quality"/>
                <owl:someValuesFrom rdf:resource="#CStorage"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#CarbonCyclingLandUseChanges -->

    <owl:Class rdf:about="#CarbonCyclingLandUseChanges">
        <rdfs:subClassOf rdf:resource="&ecology;MaterialFlow"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Changes in land use or land cover that affect carbon sequestration and storage dynamics.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#CarbonCyclingProcesses -->

    <owl:Class rdf:about="#CarbonCyclingProcesses">
        <rdfs:subClassOf rdf:resource="&ecology;MaterialEnergyFlow"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >The processes that move carbon between parts of the biosphere, the lithosphere, and the atmosphere.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#CarbonSequesteringWetland -->

    <owl:Class rdf:about="#CarbonSequesteringWetland">
        <rdfs:subClassOf rdf:resource="#Wetland"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&ecology;dependsOnProcess"/>
                <owl:someValuesFrom rdf:resource="#Methanogenesis"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&ecology;dependsOnProcess"/>
                <owl:someValuesFrom rdf:resource="#Respiration"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&ecology;dependsOnProcess"/>
                <owl:someValuesFrom rdf:resource="#SedimentTrappingAndRelease"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&ecology;dependsOnProcess"/>
                <owl:someValuesFrom rdf:resource="#Photosynthesis"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Some wetland types have limited methanogenesis. Carbon sequestering wetlands have greater rates of photosynthesis and carbon storage than methanogenesis and respiration.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#CarbonSequestrationAndStorage -->

    <owl:Class rdf:about="#CarbonSequestrationAndStorage">
        <rdfs:subClassOf rdf:resource="&eserv;EcosystemService"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&eserv;producesBenefit"/>
                <owl:allValuesFrom rdf:resource="#ClimateStability"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Storage (a stock) and sequestration (a flow) of carbon.  Both must be accounted for.  This service is for global carbon sequestration and storage, and does not include regional or local microcliamte regulation benefits, which would be classified elsewhere. (SPLIT THESE INTO SEQUESTRATION AND STORAGE?)</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#CarbonUptakeAndStorage -->

    <owl:Class rdf:about="#CarbonUptakeAndStorage">
        <rdfs:subClassOf rdf:resource="&eserv;Performance"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >(THIS IS A TRANSFORMATIVE PROCESS, NOT PERFORMANCE) Transformation of atmospheric carbon into terrestrial, aquatic, or oceanic carbon stored temporarily or permanently in vegetation, soils, geological formations, or the ocean. (SPLIT THESE INTO SEQUESTRATION AND STORAGE?)</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#ClimateChange -->

    <owl:Class rdf:about="#ClimateChange">
        <rdfs:subClassOf rdf:resource="&ecology;DirectedInteraction"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Changes in climate due to increases in atmospheric greenhouse gas concentrations.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#ClimateExtremeMitigation -->

    <owl:Class rdf:about="#ClimateExtremeMitigation">
        <rdfs:subClassOf rdf:resource="&eserv;ConservativeExploitation"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >&quot;Proper&quot; concentrations of greenhouse gases in the atmosphere keep Earth&#39;s climate from being too cold or too hot.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#ClimateStability -->

    <owl:Class rdf:about="#ClimateStability">
        <rdfs:subClassOf rdf:resource="&eserv;EcosystemBenefit"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&eserv;hasProvision"/>
                <owl:allValuesFrom rdf:resource="#CarbonUptakeAndStorage"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&eserv;hasUsage"/>
                <owl:allValuesFrom rdf:resource="#ClimateExtremeMitigation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="&xsd;string"
            >A stable climate within comfortable range for survival of humans and the species (i.e., food and others) on which they depend.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#CropRotation -->

    <owl:Class rdf:about="#CropRotation">
        <rdfs:subClassOf rdf:resource="#AgriculturalPractices"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Crop rotation can result in increased organic matter and incorporation of plant residues into the soil, leading to greater soil carbon storage.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#Cropland -->

    <owl:Class rdf:about="#Cropland">
        <rdfs:subClassOf rdf:resource="#Agriculture"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Cropland typically loses carbon due to tilling and erosion, but various agricultural practices can increase soil carbon sequestration and storage either temporarily or permanently.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#DeadVegCPool -->

    <owl:Class rdf:about="#DeadVegCPool">
        <rdfs:subClassOf rdf:resource="#BiosphereCPool"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Dead vegetation, including snags and coarse woody debris.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#Decomposition -->

    <owl:Class rdf:about="#Decomposition">
        <rdfs:subClassOf rdf:resource="#CarbonCyclingProcesses"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Decay of dead organic matter releases terrestrial (biosphere) carbon into the atmosphere.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#DecompositionRate -->

    <owl:Class rdf:about="#DecompositionRate">
        <rdfs:subClassOf rdf:resource="&habitat;SoilQuality"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&DOLCE-Lite;generically-dependent-on"/>
                <owl:someValuesFrom rdf:resource="#OxygenConditions"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Affected mainly by climate (already included?)</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#Deforestation -->

    <owl:Class rdf:about="#Deforestation">
        <rdfs:subClassOf rdf:resource="#CarbonCyclingLandUseChanges"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Removal of trees from a region, generally as raw materials or to clear the land for other uses.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#DeltaicSubsidence -->

    <owl:Class rdf:about="#DeltaicSubsidence">
        <rdfs:subClassOf rdf:resource="#GeologicSequestration"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >The natural subsidence of deltas and large inland water bodies (i.e., Mississippi Delta, Black Sea) moves carbon from the atmosphere to the lithosphere.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#Depth -->

    <owl:Class rdf:about="#Depth">
        <rdfs:subClassOf rdf:resource="&habitat;SoilQuality"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Less carbon can be stored in soils that are shallow, versus those with deep soil horizons.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#DiverseForest -->

    <owl:Class rdf:about="#DiverseForest">
        <rdfs:subClassOf rdf:resource="#CStoringOrSequesteringHabitat"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Forests are likely to store more of their carbon aboveground, although some forests have substantial soil carbon storage as well.  Multispecies forests provide a mix of ecosystem services, including carbon sequestration and storage.  For Madagascar, the succession process is slow even in humid forests (greater than 50 years for forestry purposes, to say nothing of other ecosystem services&#39; recovery).</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#DurableWoodProductsCPool -->

    <owl:Class rdf:about="#DurableWoodProductsCPool">
        <rdfs:subClassOf rdf:resource="#BiosphereCPool"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Furniture, books, etc. that &quot;permanently&quot; store carbon (unless there&#39;s a fire)</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#Early -->

    <owl:Class rdf:about="#Early">
        <rdfs:subClassOf rdf:resource="#SuccessionalStage"/>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#EarlySuccessionalDiverseForest -->

    <owl:Class rdf:about="#EarlySuccessionalDiverseForest">
        <rdfs:subClassOf rdf:resource="#DiverseForest"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Multispecies forest dominated by young trees.  Generally high rates of carbon sequestration, low storage.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#EarlySuccessionalPlantation -->

    <owl:Class rdf:about="#EarlySuccessionalPlantation">
        <rdfs:subClassOf rdf:resource="#Plantation"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Plantation dominated by young trees.  Generally high rates of carbon sequestration, low storage.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#Elevation -->

    <owl:Class rdf:about="#Elevation">
        <rdfs:subClassOf rdf:resource="&habitat;HabitatQuality"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >A region&#39;s elevation affects its climate, hence the vegetation types that can grow there.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#Fertilization -->

    <owl:Class rdf:about="#Fertilization">
        <rdfs:subClassOf rdf:resource="#AgriculturalPractices"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Organic or chemical fertilizers, and the rates and timing when they are applied, can impact soil carbon sequestration rates.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#Fire -->

    <owl:Class rdf:about="#Fire">
        <rdfs:subClassOf rdf:resource="#CarbonCyclingLandUseChanges"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Fire events.  Fire converts carbon in living vegetation and soil to atmospheric CO2.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#FossilFuelBurning -->

    <owl:Class rdf:about="#FossilFuelBurning">
        <rdfs:subClassOf rdf:resource="#AnthropogenicRelease"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Extraction and burning of fossil fuels moves carbon from the lithosphere to the atmosphere.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#GeologicSequestration -->

    <owl:Class rdf:about="#GeologicSequestration">
        <rdfs:subClassOf rdf:resource="#CarbonCyclingProcesses"/>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#GrasslandHerbaceous -->

    <owl:Class rdf:about="#GrasslandHerbaceous">
        <rdfs:subClassOf rdf:resource="#CStoringOrSequesteringHabitat"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Herbaceous systems (i.e., grasslands) may store relatively more of their carbon in the soil, and are likely to have less aboveground carbon storage.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#GrazingIntensity -->

    <owl:Class rdf:about="#GrazingIntensity">
        <rdfs:subClassOf rdf:resource="#AgriculturalPractices"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Grazing intensity can change the plant community and soil characteristics, altering both carbon sequestration and storage.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#GrowingSeason -->

    <owl:Class rdf:about="#GrowingSeason">
        <rdfs:subClassOf rdf:resource="&habitat;ClimateQuality"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Length of time between last and first frosts where the vast majority of plant photosynthesis and growth occurs.  Warmer climates with longer growing seasons would be expected to have greater plant growth, thus to harbor ecosystems with greater carbon sequestration and storage.  This has been demonstrated for agricultural soil carbon storage (Ogle et al. 2005).</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#GrowingSeasonLength -->

    <owl:Class rdf:about="#GrowingSeasonLength">
        <rdfs:subClassOf rdf:resource="#GrowingSeason"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >A longer growing season (typically at lower elevations and latitudes) generally leads to ecosystems capable of sequestering and storing more carbon.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#IncreasedDroughtLikelihood -->

    <owl:Class rdf:about="#IncreasedDroughtLikelihood">
        <rdfs:subClassOf rdf:resource="#ClimateChange"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#leadsToLess"/>
                <owl:someValuesFrom rdf:resource="#RainfallAmount"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#leadsToMore"/>
                <owl:someValuesFrom rdf:resource="#Decomposition"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#leadsToLess"/>
                <owl:someValuesFrom rdf:resource="&habitat;PrimaryProductivity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Drought may change the long-term distribution of ecosystems such as forests or grasslands.  Drier and/or warmer conditions may also lower the water table in wetlands, allowing oxidation of stored soil carbon in wetlands.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#IncreasedForestDroughtDiseaseRisk -->

    <owl:Class rdf:about="#IncreasedForestDroughtDiseaseRisk">
        <rdfs:subClassOf rdf:resource="#ClimateChange"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#leadsToLess"/>
                <owl:someValuesFrom rdf:resource="&habitat;PrimaryProductivity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Forests may be subject to greater drought and disease, reducing their ability to provide various ecosystem services.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#IncreasedForestFirePotential -->

    <owl:Class rdf:about="#IncreasedForestFirePotential">
        <rdfs:subClassOf rdf:resource="#ClimateChange"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#leadsToMore"/>
                <owl:someValuesFrom rdf:resource="#Natural"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Fires will limit the ability of forests to store carbon.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#IncreasedPermafrostMelt -->

    <owl:Class rdf:about="#IncreasedPermafrostMelt">
        <rdfs:subClassOf rdf:resource="#ClimateChange"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Melting permafrost may release methane stored in permafrost thaw lakes, leading to increased methane release (Walter et al. 2006).</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#Irrigation -->

    <owl:Class rdf:about="#Irrigation">
        <rdfs:subClassOf rdf:resource="#AgriculturalPractices"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Irrigation can increase plant growth, potentially leading to greater soil carbon sequestration rates.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#LandUseChange -->

    <owl:Class rdf:about="#LandUseChange">
        <rdfs:subClassOf rdf:resource="#CarbonCyclingLandUseChanges"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Wider, landscape-scale land use change that alters carbon sequestration and storage dynamics.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#Late -->

    <owl:Class rdf:about="#Late">
        <rdfs:subClassOf rdf:resource="#SuccessionalStage"/>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#LateSuccessionalDiverseForest -->

    <owl:Class rdf:about="#LateSuccessionalDiverseForest">
        <rdfs:subClassOf rdf:resource="#DiverseForest"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Multispecies forest dominated by older trees.  May include mixed age classes as local disturbance creates openings for young trees.  Generally low rates of carbon sequestration but high carbon storage.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#LateSuccessionalPlantation -->

    <owl:Class rdf:about="#LateSuccessionalPlantation">
        <rdfs:subClassOf rdf:resource="#Plantation"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Plantation dominated by older trees.  Generally low rates of carbon sequestration but high carbon storage.  Often harvested before spending too long in this successional state.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#LeafCPool -->

    <owl:Class rdf:about="#LeafCPool">
        <rdfs:subClassOf rdf:resource="#VegetativeCPool"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Dependent on volume, leaf type, seasonality.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#LithosphereCPool -->

    <owl:Class rdf:about="#LithosphereCPool">
        <rdfs:subClassOf rdf:resource="&ecology;MaterialStock"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&ecology;dependsOnProcess"/>
                <owl:someValuesFrom rdf:resource="#GeologicSequestration"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&ecology;dependsOnProcess"/>
                <owl:someValuesFrom rdf:resource="#FossilFuelBurning"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Carbon deposits in the earth&#39;s crust, including fossil fuels (i.e., coal, oil, natural gas, tar sands).  Also includes carbon being naturally moved into the crust by geologic processes (i.e., along naturally subsiding river deltas).</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#Managed -->

    <owl:Class rdf:about="#Managed">
        <rdfs:subClassOf rdf:resource="&habitat;FireRegime"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Areas where fire is used by humans to manage the system.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#MeanGrowingSeasonTemp -->

    <owl:Class rdf:about="#MeanGrowingSeasonTemp">
        <rdfs:subClassOf rdf:resource="#GrowingSeason"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Climates with same growing season length can still have a warmer vs. colder climate (i.e., oceanic vs. continental climates).</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#MeanOceanTemp -->

    <owl:Class rdf:about="#MeanOceanTemp">
        <rdfs:subClassOf rdf:resource="#OceanQuality"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Plus other oceanic variables affecting ocean carbon exchange?  Does temp affect it?</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#Methanogenesis -->

    <owl:Class rdf:about="#Methanogenesis">
        <rdfs:subClassOf rdf:resource="#CarbonCyclingProcesses"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&DOLCE-Lite;generically-dependent-on"/>
                <owl:someValuesFrom rdf:resource="#OxygenConditions"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="&xsd;string"
            >The release of methane from hydrates or permafrost, or its production (methanogenesis), usually in anaerobic environments like wetlands.  Methane is a powerful greenhouse gas.  This process transfers carbon from the biosphere to the atmosphere.  Mitra et al. (2005) provide methanogenesis rates for global wetlands.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#Mid -->

    <owl:Class rdf:about="#Mid">
        <rdfs:subClassOf rdf:resource="#SuccessionalStage"/>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#MidSuccessionalDiverseForest -->

    <owl:Class rdf:about="#MidSuccessionalDiverseForest">
        <rdfs:subClassOf rdf:resource="#DiverseForest"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Multispecies forest dominated by middle aged trees.  Generally high to moderate rates of carbon sequestration, moderate to high storage.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#MidSuccessionalPlantation -->

    <owl:Class rdf:about="#MidSuccessionalPlantation">
        <rdfs:subClassOf rdf:resource="#Plantation"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Plantation dominated by middle aged trees.  Generally high to moderate rates of carbon sequestration, moderate to high storage.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#Natural -->

    <owl:Class rdf:about="#Natural">
        <rdfs:subClassOf rdf:resource="&habitat;FireRegime"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Areas where fires burn on a &quot;natural&quot; basis (these may not exist any more).</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#NonCarbonSequesteringWetland -->

    <owl:Class rdf:about="#NonCarbonSequesteringWetland">
        <rdfs:subClassOf rdf:resource="#Wetland"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&ecology;dependsOnProcess"/>
                <owl:someValuesFrom rdf:resource="#SedimentTrappingAndRelease"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&ecology;dependsOnProcess"/>
                <owl:someValuesFrom rdf:resource="#Photosynthesis"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&ecology;dependsOnProcess"/>
                <owl:someValuesFrom rdf:resource="#Methanogenesis"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&ecology;dependsOnProcess"/>
                <owl:someValuesFrom rdf:resource="#Respiration"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Many wetlands may have negligable overall greenhouse gas balance - although they may sequester carbon, they produce methane, a powerful greenhouse gas.  Non-carbon sequestering wetlands have greater rates of methanogenesis and respiration than photosynthesis and carbon storage.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#Ocean -->

    <owl:Class rdf:about="#Ocean">
        <rdfs:subClassOf rdf:resource="#CStoringOrSequesteringHabitat"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >(Need to better identify the oceanic processes involved in carbon storage/uptake)</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#OceanQuality -->

    <owl:Class rdf:about="#OceanQuality">
        <rdfs:subClassOf rdf:resource="&habitat;ClimateQuality"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Oceanic factors affecting C exchange between atmosphere and ocean C pools.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#OceanUptake -->

    <owl:Class rdf:about="#OceanUptake">
        <rdfs:subClassOf rdf:resource="#CarbonCyclingProcesses"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&DOLCE-Lite;generically-dependent-on"/>
                <owl:someValuesFrom rdf:resource="#SurfaceRoughness"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&DOLCE-Lite;generically-dependent-on"/>
                <owl:someValuesFrom rdf:resource="#OceanpH"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="&xsd;string"
            >The exchange of carbon between the atmosphere and ocean (biosphere), including both its uptake and release by the oceans.  CO2 diffuses into water at the ocean surface, forming carbonic acid (CO3).  This process is dependent on ocean pH and surface roughness (i.e., wind).</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#OceanicCPool -->

    <owl:Class rdf:about="#OceanicCPool">
        <rdfs:subClassOf rdf:resource="#BiosphereCPool"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&ecology;dependsOnProcess"/>
                <owl:someValuesFrom rdf:resource="#OceanUptake"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Carbon dissolved in ocean water, plus as part of creatures with shells, coral reefs, oceanic limestones.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#OceanpH -->

    <owl:Class rdf:about="#OceanpH">
        <rdfs:subClassOf rdf:resource="#OceanQuality"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >The ocean&#39;s pH dictates how much additional CO2 can be absorped.  As the oceans grow more acidic, rates of oceanic CO2 uptake are expected to slow.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#OrganicMatterDensity -->

    <owl:Class rdf:about="#OrganicMatterDensity">
        <rdfs:subClassOf rdf:resource="&habitat;SoilQuality"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Affects soil C storage capacity.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#OxygenConditions -->

    <owl:Class rdf:about="#OxygenConditions">
        <rdfs:subClassOf rdf:resource="&habitat;SoilQuality"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >The presence of oxygen (aerobic environment) allows respiration to take place, where sugars are oxidized and CO2 is produced.  In anaerobic environments, methanogenesis may occur.  Falling water tables in wetlands can also allow oxidation of soils, reducing their carbon storage.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#Pasture -->

    <owl:Class rdf:about="#Pasture">
        <rdfs:subClassOf rdf:resource="#Agriculture"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Areas grazed by domestic animals.  Depending on its intensity, grazing may alter plant species composition.  Extensive grazing can also lead to soil compaction and erosion.  These changes to the plant community and soil characteristics can change soil carbon sequestration and storage.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#Peatlands -->

    <owl:Class rdf:about="#Peatlands">
        <rdfs:subClassOf rdf:resource="#CarbonSequesteringWetland"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Peatlands store substantial carbon in the soil as carbon-rich peat.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#Permafrost -->

    <owl:Class rdf:about="#Permafrost">
        <rdfs:subClassOf rdf:resource="#CStoringOrSequesteringHabitat"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Permafrost may store carbon in the form of methane, as decomposition is inhibited by cold and/or low oxygen conditions.  Warming may lead to methane release from these systems (Walter et al. 2006).</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#Photosynthesis -->

    <owl:Class rdf:about="#Photosynthesis">
        <rdfs:subClassOf rdf:resource="#CarbonCyclingProcesses"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >The photosynthetic uptake of atmospheric CO2 by plants, which moves carbon from the atmosphere to the biosphere.  Plants store carbon in organic molecules that may eventually be incorporated into the soil and animal biomass.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#PlantSuccession -->

    <owl:Class rdf:about="#PlantSuccession">
        <rdfs:subClassOf rdf:resource="#CarbonCyclingLandUseChanges"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >The gradual natural process of species turnover and community change, generally following disturbance.  Depending on the biome, disturbance type and intensity, this may include natural regrowth of trees.  In Madagascar, this is generally a slow process.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#Plantation -->

    <owl:Class rdf:about="#Plantation">
        <rdfs:subClassOf rdf:resource="#CStoringOrSequesteringHabitat"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Forests are likely to store more of their carbon aboveground, although some forests have substantial soil carbon storage as well.  Plantations may have greater carbon sequestration rates, but often lower values for other ecosystem services, such as habitat- and water-related services.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#Rainfall -->

    <owl:Class rdf:about="#Rainfall">
        <rdfs:subClassOf rdf:resource="&habitat;ClimateQuality"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Rainfall influences net primary productivity, a key indicator of sequestration and storage potential.  Plants in wetter cliamtes can generally grow faster (sequestering more carbon), but also grow larger (store more carbon).  This has been demonstrated for agricultural soil carbon storage (Ogle et al. 2005).</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#RainfallAmount -->

    <owl:Class rdf:about="#RainfallAmount">
        <rdfs:subClassOf rdf:resource="#Rainfall"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Total average amount of yearly rainfall.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#RainfallClassA -->

    <owl:Class rdf:about="#RainfallClassA">
        <rdfs:label
            >400 mm/y or less</rdfs:label>
        <rdfs:subClassOf rdf:resource="#RainfallClassification12"/>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonEcology.owl#RainfallClassB -->

    <owl:Class rdf:about="#RainfallClassB">
        <rdfs:label>400-600 mm/y</rdfs:label>
        <rdfs:subClassOf rdf:resource="#RainfallClassification12"/>
