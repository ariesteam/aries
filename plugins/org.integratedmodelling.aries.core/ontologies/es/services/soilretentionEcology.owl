      <owl:Class rdf:about="http://www.integratedmodelling.org/ks/ecology/eco/habitat.owl#Accessibility"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Grassland">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Areas dominated by grammanoid or herbaceous vegetation, generally greater than 80% of total vegetation. These areas are not subject to intensive management such as tilling, but can be utilized for grazing (MRLC 2006).</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#SoilRetainingHabitat"/>
  </owl:Class>
  <owl:Class rdf:about="http://www.integratedmodelling.org/ks/es/eserv.owl#AbsoluteSink"/>
  <owl:Class rdf:about="#CoastalWetlands">
    <rdfs:subClassOf rdf:resource="#HabitatTypes"/>
  </owl:Class>
  <owl:Class rdf:ID="LowSnowpack">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#MonthlySnowpack"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#VegetativeMaturity">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Vegetation composition  beyond which more or less full hydrologic services are provided.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="#PercentVegetationCover"/>
        <owl:onProperty rdf:resource="http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#generically-dependent-on"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="#CanopyLayering"/>
        <owl:onProperty rdf:resource="http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#generically-dependent-on"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="#RootDensity"/>
        <owl:onProperty rdf:resource="http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#generically-dependent-on"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/ecology/eco/habitat.owl#EcosystemQuality"/>
  </owl:Class>
  <owl:Class rdf:ID="LowPrecipitation">
    <rdfs:subClassOf rdf:resource="#MonthlyPrecipitation"/>
  </owl:Class>
  <owl:Class rdf:ID="NoSedimentSink">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#SedimentSink"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#ClimateChange">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Changes in climate due to increases in atmospheric greenhouse gas concentrations.</rdfs:comment>
