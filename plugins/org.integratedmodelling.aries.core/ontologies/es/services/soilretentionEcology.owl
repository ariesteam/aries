<?xml version="1.0"?>
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:ecology="http://www.integratedmodelling.org/ks/ecology/eco/ecology.owl#"
    xmlns:habitat="http://www.integratedmodelling.org/ks/ecology/eco/habitat.owl#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:thinklab-core="http://www.integratedmodelling.org/ks/thinklab/thinklab-core.owl#"
    xmlns:owl2xml="http://www.w3.org/2006/12/owl2-xml#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:soilretentionEcology="http://www.integratedmodelling.org/ks/es/soilretentionEcology#"
    xmlns:DOLCE-Lite="http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#"
    xmlns:eserv="http://www.integratedmodelling.org/ks/es/eserv.owl#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xml:base="http://www.integratedmodelling.org/ks/es/soilretentionEcology">
  <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="http://www.integratedmodelling.org/ks/es/eserv.owl"/>
    <owl:imports rdf:resource="http://www.integratedmodelling.org/ks/ecology/eco/habitat.owl"/>
  </owl:Ontology>
  <owl:Class rdf:ID="IncreasedStormRainfall">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Larger storms with more precipitation will increase the intensity of flood events.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:ID="Amount"/>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="leadsToMore"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="ClimateChange"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="RainfallRunoffErosivityIndex">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >For use in (Revised) Universal Soil Loss Equation.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Precipitation"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Slope">
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://www.integratedmodelling.org/ks/ecology/eco/habitat.owl#Slope"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="SoilOrganicMatter">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Content and types of organic versus inorganic matter in the soil.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://www.integratedmodelling.org/ks/ecology/eco/habitat.owl#SoilQuality"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Rivers">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="HabitatTypes"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Temperature">
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://www.integratedmodelling.org/ks/ecology/eco/habitat.owl#ClimateQuality"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="NoSnowmelt">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="MonthlySnowmelt"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#HabitatTypes">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="LandUseLandCover"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="LowerWatershed">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="WatershedPosition"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="SeagrassAbsent">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Seagrass"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="VegetationType">
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://www.integratedmodelling.org/ks/ecology/eco/habitat.owl#EcosystemQuality"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Reservoirs">
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#dependent-place"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="LowSoilErodibility">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="SoilErodibility"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="ModeratePopulationDensity">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="PopulationDensity"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Construction">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Construction sites include areas of exposed soil that are highly susceptible to erosion.  Over the long term, increasing impervious area can  lead to alteration of stream geomorphic equilibrium, leading to downcutting, bank erosion, and downstream movement of sediment.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:ID="CanopyLayering"/>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="leadsToLess"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:ID="PercentVegetationCover"/>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#leadsToLess"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:ID="RootDensity"/>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#leadsToLess"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://www.integratedmodelling.org/ks/ecology/eco/ecology.owl#MaterialFlow"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="MidSuccessionalPlantation">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Plantation dominated by middle aged trees.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Plantation"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="HighSnowpack">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="MonthlySnowpack"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="LateSuccessionalForest">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Multispecies forest dominated by older trees.  May include mixed age classes as local disturbance creates openings for young trees.  Multilayer canopy and high vegetative cover and root densities are likely to provide maximum soil retention.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="DiverseForest"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="CoarseSoilTexture">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="SoilTexture"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="DamAbsent">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Dams"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="NoSuccession">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="SuccessionalStage"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="GentlyUndulating">
    <rdfs:subClassOf rdf:resource="#Slope"/>
  </owl:Class>
  <owl:Class rdf:ID="HighTemperature">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="MonthlyTemperature"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#Dams">
    <rdfs:subClassOf rdf:resource="http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#dependent-place"/>
  </owl:Class>
  <owl:Class rdf:ID="RollingToHilly">
    <rdfs:subClassOf rdf:resource="#Slope"/>
  </owl:Class>
  <owl:Class rdf:ID="Wind">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    ></rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="SoilErosionForces"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#PopulationDensity">
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://www.integratedmodelling.org/ks/ecology/eco/habitat.owl#HumanPopulation"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="CoastlinePresent">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Coastline"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="MangrovesAbsent">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Mangroves"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="VeryHighRunoff">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Runoff"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="NoFishermanUse">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="FishermenUse"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="MidSuccessionalForest">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Multispecies forest dominated by middle aged trees.   Multilayer canopy and relatively high vegetative cover and root densities are likely to provide relatively high soil retention services.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#DiverseForest"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="SoilRetainingHabitat">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >An ecosystem that retains soil in situ, usually through a combination of a vegetative canopy that intercepts rainfall and root networks that hold soil in place.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom>
          <owl:Class rdf:ID="SoilRetentionCapacity"/>
        </owl:allValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#has-quality"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#LandUseLandCover"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="LowStreamGradient">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="StreamGradient"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="LowSnowmelt">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#MonthlySnowmelt"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="IncreasedForestDroughtDiseaseRisk">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Forests may be subject to greater drought and disease, reducing their ability to provide various ecosystem services.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:ID="VegetativeMaturity"/>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#leadsToLess"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#ClimateChange"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="SeagrassPresent">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Seagrass"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="HighSedimentSource">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="SedimentSourceValue"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Deforestation">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Removal of trees from a region, generally as raw materials or to clear the land for other uses.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:about="#PercentVegetationCover"/>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#leadsToLess"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:about="#RootDensity"/>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#leadsToLess"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:about="#CanopyLayering"/>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#leadsToLess"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/ecology/eco/ecology.owl#MaterialFlow"/>
  </owl:Class>
  <owl:Class rdf:ID="HighPopulationDensity">
    <rdfs:subClassOf rdf:resource="#PopulationDensity"/>
  </owl:Class>
  <owl:Class rdf:about="#Coastline">
    <rdfs:subClassOf rdf:resource="#HabitatTypes"/>
  </owl:Class>
  <owl:Class rdf:ID="ConservationPractice">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >For use in (Revised) Universal Soil Loss Equation.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://www.integratedmodelling.org/ks/ecology/eco/habitat.owl#ManagementStructure"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="ReefAbsent">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Reefs"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="LowVegetationCover">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#PercentVegetationCover"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#SuccessionalStage">
    <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/ecology/eco/habitat.owl#EcosystemQuality"/>
  </owl:Class>
  <owl:Class rdf:ID="VeryHighFloodplainVetgetationCover">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="FloodplainVegetationCover"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="VeryNarrowFloodplain">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="FloodplainWidth"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="HighSoilErodibility">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#SoilErodibility"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="ModerateSedimentSink">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="SedimentSink"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="LowTemperature">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#MonthlyTemperature"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="CommercialForestryPractices">
    <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/ecology/eco/habitat.owl#ManagementStructure"/>
  </owl:Class>
  <owl:Class rdf:ID="CoverManagement">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >For use in (Revised) Universal Soil Loss Equation.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/ecology/eco/habitat.owl#ManagementStructure"/>
  </owl:Class>
  <owl:Class rdf:ID="ModerateVegetationCover">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#PercentVegetationCover"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="BridgePresent">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Bridges"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Lakes">
    <rdfs:subClassOf rdf:resource="#HabitatTypes"/>
  </owl:Class>
  <owl:Class rdf:ID="TropicalStormProbability">
    <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/ecology/eco/habitat.owl#ClimateQuality"/>
  </owl:Class>
  <owl:Class rdf:ID="RiversPresent">
    <rdfs:subClassOf rdf:resource="#Rivers"/>
  </owl:Class>
  <owl:Class rdf:about="#MonthlyTemperature">
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://www.integratedmodelling.org/ks/ecology/eco/habitat.owl#AtmosphericTemperature"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Seasonality">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Distribution of rainfall throughout the year (i.e., are there rainy and dry seasons, or is it more evenly distributed?).  Notably in Madagascar, eastern Madagascar recieves monsoon rainfall from November to April.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Precipitation"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="SoilErosionProcesses">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Soil erosion can occur as a result of one or several processes acting together</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/ecology/eco/ecology.owl#MaterialFlow"/>
  </owl:Class>
  <owl:Class rdf:ID="Afforestation">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Replanting of trees by humans, with the goal of increasing forest cover.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:about="#PercentVegetationCover"/>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#leadsToMore"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:about="#RootDensity"/>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#leadsToMore"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/ecology/eco/ecology.owl#MaterialFlow"/>
  </owl:Class>
  <owl:Class rdf:about="http://www.integratedmodelling.org/ks/ecology/eco/habitat.owl#SocioEconomicQuality"/>
  <owl:Class rdf:ID="ReefPresent">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Reefs"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="VeryLowPrecipitation">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="MonthlyPrecipitation"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="HighSnowmelt">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#MonthlySnowmelt"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#SedimentSourceValue">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Source"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="LakesAbsent">
    <rdfs:subClassOf rdf:resource="#Lakes"/>
  </owl:Class>
  <owl:Class rdf:about="#PercentVegetationCover">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Percent vegetative cover.   Different vegetation types also have different rooting depths, root densities, and transpiration rates, affecting their ability to aid in infiltration of water and/or slowing the erosive energy of water or wind.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/ecology/eco/habitat.owl#EcosystemQuality"/>
  </owl:Class>
  <owl:Class rdf:ID="Levees">
    <rdfs:subClassOf rdf:resource="http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#dependent-place"/>
  </owl:Class>
  <owl:Class rdf:ID="WideFloodplain">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#FloodplainWidth"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Gullying">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >An accelerating process, where erosion generates gullies that grow in size and carry away increasing amounts of soil.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#SoilErosionProcesses"/>
  </owl:Class>
  <owl:Class rdf:ID="CropsBarrenDeveloped">
    <rdfs:subClassOf rdf:resource="#VegetationType"/>
  </owl:Class>
  <owl:Class rdf:ID="MidSuccession">
    <rdfs:subClassOf rdf:resource="#SuccessionalStage"/>
  </owl:Class>
  <owl:Class rdf:ID="EarlySuccessionalPlantation">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Plantation dominated by young trees.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Plantation"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="SoilParentMaterial">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >The bedrock material whose breakdown helps generate soil.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/ecology/eco/habitat.owl#SoilQuality"/>
  </owl:Class>
  <owl:Class rdf:ID="NoSedimentSource">
    <rdfs:subClassOf rdf:resource="#SedimentSourceValue"/>
  </owl:Class>
  <owl:Class rdf:ID="LowPopulationDensity">
    <rdfs:subClassOf rdf:resource="#PopulationDensity"/>
  </owl:Class>
  <owl:Class rdf:ID="NoVegetativeMaturity">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#VegetativeMaturity"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="EarlySuccessionalForest">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Multispecies forest dominated by young trees.  The recent legacy of disturbance, combined with less well developed canopy layers, vegetative cover, and root densities, are likely to lead to relatively lower soil retention.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#DiverseForest"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="VeryHighPrecipitation">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#MonthlyPrecipitation"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="UpperWatershed">
