    <rdfs:subClassOf rdf:resource="#Snowmelt"/>
  </owl:Class>
  <owl:Class rdf:ID="LowSedimentSink">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#SedimentSink"/>
    </rdfs:subClassOf>
