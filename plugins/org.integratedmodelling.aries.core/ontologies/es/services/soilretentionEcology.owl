    <rdfs:subClassOf rdf:resource="#SedimentSink"/>
  </owl:Class>
  <owl:Class rdf:ID="InFloodplain">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Area next to a stream that is inundated by water from a flood.  An X year floodplain has a (1/X)% chance each year of being equaled or exceeded.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#Floodplain"/>
  </owl:Class>
  <owl:Class rdf:ID="NavigationInfrastructureAbsent">
    <rdfs:subClassOf rdf:resource="#NavigationInfrastructure"/>
  </owl:Class>
  <owl:Class rdf:ID="LowFloodplainVegetationCover">
    <rdfs:subClassOf rdf:resource="#FloodplainVegetationCover"/>
  </owl:Class>
  <owl:Class rdf:ID="HighSlopeStability">
    <rdfs:subClassOf rdf:resource="#SlopeStability"/>
  </owl:Class>
  <owl:Class rdf:ID="ModerateSoilErodibility">
    <rdfs:subClassOf rdf:resource="#SoilErodibility"/>
  </owl:Class>
  <owl:Class rdf:about="#SoilDepthToBedrock">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >The depth of soils before hitting bedrock.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/ecology/eco/habitat.owl#SoilQuality"/>
  </owl:Class>
