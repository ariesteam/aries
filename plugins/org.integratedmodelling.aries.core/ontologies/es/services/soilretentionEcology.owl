    <rdfs:subClassOf rdf:resource="#SuccessionalStage"/>
  </owl:Class>
  <owl:Class rdf:ID="ModerateSlopeStability">
    <rdfs:subClassOf rdf:resource="#SlopeStability"/>
  </owl:Class>
  <owl:Class rdf:ID="SoilLoss">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Total soil loss</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/ecology/eco/ecology.owl#MaterialFlow"/>
  </owl:Class>
  <owl:Class rdf:about="#NavigationInfrastructure">
    <rdfs:subClassOf rdf:resource="http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#dependent-place"/>
  </owl:Class>
