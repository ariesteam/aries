  </owl:Class>
  <owl:Class rdf:ID="MangrovesPresent">
    <rdfs:subClassOf rdf:resource="#Mangroves"/>
  </owl:Class>
  <owl:ObjectProperty rdf:about="#matters-in-event-of">
    <rdfs:domain rdf:resource="#SoilErodibility"/>
  </owl:ObjectProperty>
