    <rdfs:subClassOf rdf:resource="#PopulationDensity"/>
  </owl:Class>
  <owl:Class rdf:ID="SalmonSpawningGroundsPresent">
    <rdfs:subClassOf rdf:resource="#SalmonSpawningGrounds"/>
  </owl:Class>
  <owl:Class rdf:ID="MiddleWatershed">
    <rdfs:subClassOf rdf:resource="#WatershedPosition"/>
  </owl:Class>
  <owl:Class rdf:about="http://www.integratedmodelling.org/ks/ecology/eco/habitat.owl#Slope">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Slope influences the speed of surface flow, hence the erosive power of water.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#SedimentSink">
    <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/es/eserv.owl#AbsoluteSink"/>
  </owl:Class>
  <owl:Class rdf:ID="ForestGrasslandWetland">
    <rdfs:subClassOf rdf:resource="#VegetationType"/>
  </owl:Class>
