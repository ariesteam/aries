    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonService.owl#GoodConditionsForFoodFiberWater -->

    <owl:Class rdf:about="#GoodConditionsForFoodFiberWater">
        <rdfs:subClassOf rdf:resource="#ClimateStability"/>
