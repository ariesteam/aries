    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonService.owl#GreenhouseGasEmitters -->

    <owl:Class rdf:about="#GreenhouseGasEmitters">
        <rdfs:subClassOf rdf:resource="&eserv;HumanConsumer"/>
        <rdfs:comment
