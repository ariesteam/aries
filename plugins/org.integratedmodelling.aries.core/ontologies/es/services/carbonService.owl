    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonService.owl#HydroelectricDams -->

    <owl:Class rdf:about="#HydroelectricDams">
        <rdfs:subClassOf rdf:resource="&eserv;ContingentPolicyTarget"/>
    </owl:Class>
    


