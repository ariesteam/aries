    <!-- http://www.integratedmodelling.org/ks/es/carbonService.owl#ObjectProperty_1 -->

    <owl:ObjectProperty rdf:about="#ObjectProperty_1">
        <rdfs:domain rdf:resource="#AvoidedSeaLevelRise"/>
    </owl:ObjectProperty>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonService.owl#isBeneficialFor -->

    <owl:ObjectProperty rdf:about="#isBeneficialFor">
        <rdfs:comment rdf:datatype="&xsd;string"
            >Identifies the beneficiary for each benefit.</rdfs:comment>
        <rdfs:domain rdf:resource="&eserv;EcosystemBenefit"/>
