<?xml version="1.0"?>
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:protege="http://protege.stanford.edu/plugins/owl/protege#"
    xmlns="http://www.integratedmodelling.org/ks/es/carbonService.owl#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:owl2xml="http://www.w3.org/2006/12/owl2-xml#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:eserv="http://www.integratedmodelling.org/ks/es/eserv.owl#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xml:base="http://www.integratedmodelling.org/ks/es/carbonService.owl">
  <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="http://www.integratedmodelling.org/ks/es/eserv.owl"/>
    <owl:imports rdf:resource="http://www.integratedmodelling.org/ks/ecology/eco/habitat.owl"/>
  </owl:Ontology>
  <owl:Class rdf:ID="HydroelectricDams">
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://www.integratedmodelling.org/ks/es/eserv.owl#ContingentPolicyTarget"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="GreenhouseGasEmitters">
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://www.integratedmodelling.org/ks/es/eserv.owl#HumanConsumer"/>
    </rdfs:subClassOf>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >They benefit under a scenario of compensation</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="http://www.integratedmodelling.org/ks/es/eserv.owl#UndesiredEndurant"/>
  <owl:Class rdf:ID="FireProneHousing">
    <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/es/eserv.owl#ContingentPolicyTarget"/>
  </owl:Class>
  <owl:Class rdf:ID="ClimateExtremeMitigation">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >"Proper" concentrations of greenhouse gases in the atmosphere keep Earth's climate from being too cold or too hot.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:ID="ParticularlyVulnerableGroups"/>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="http://www.integratedmodelling.org/ks/es/eserv.owl#hasConsumer"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom>
          <owl:Class rdf:ID="AllPeopleEverywhere"/>
        </owl:allValuesFrom>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="http://www.integratedmodelling.org/ks/es/eserv.owl#hasConsumer"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://www.integratedmodelling.org/ks/es/eserv.owl#ConservativeExploitation"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="CoastalFloodZone">
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://www.integratedmodelling.org/ks/es/eserv.owl#EcosystemSupportedResource"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="FireFrequency">
    <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/es/eserv.owl#ContingentPolicyTarget"/>
  </owl:Class>
  <owl:Class rdf:ID="PotentialEvapotranspiration">
    <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/es/eserv.owl#EcosystemSupportedResource"/>
  </owl:Class>
  <owl:Class rdf:ID="SummerHighWinterLow">
    <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/es/eserv.owl#EcosystemSupportedResource"/>
  </owl:Class>
  <owl:Class rdf:ID="Forest">
    <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/es/eserv.owl#EcosystemSupportedResource"/>
  </owl:Class>
  <owl:Class rdf:ID="LandslideRiskZone">
    <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/es/eserv.owl#EcosystemSupportedResource"/>
  </owl:Class>
  <owl:Class rdf:ID="CUptake">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Transformation of atmospheric carbon into terrestrial, aquatic, or oceanic carbon stored temporarily or permanently in vegetation, soils, geological formations, or the ocean.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="http://www.integratedmodelling.org/ks/es/eserv.owl#hasProducer"/>
        </owl:onProperty>
        <owl:allValuesFrom>
          <owl:Class rdf:ID="CSequesteringEcosystem"/>
        </owl:allValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://www.integratedmodelling.org/ks/es/eserv.owl#Transformation"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="http://www.integratedmodelling.org/ks/es/eserv.owl#EcosystemService"/>
  <owl:Class rdf:ID="Survival">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="SafetyAndSecurity"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="http://www.integratedmodelling.org/ks/es/eserv.owl#EcosystemBenefit"/>
  <owl:Class rdf:ID="MountainGlaciers">
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://www.integratedmodelling.org/ks/es/eserv.owl#EcosystemGeneratedResource"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="EconomicViability">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#SafetyAndSecurity"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="HardwoodSoftwoodRatio">
    <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/es/eserv.owl#EcosystemSupportedResource"/>
  </owl:Class>
  <owl:Class rdf:ID="HabitabilityOfLand">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Some places are going to become less to non-inhabitable - LA, Bangladesh, Pacific islands - will lose economic capacity, and sheer ability to live there.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://www.integratedmodelling.org/ks/es/eserv.owl#HumanPolicyTarget"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Farmland">
    <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/es/eserv.owl#ContingentPolicyTarget"/>
  </owl:Class>
  <owl:Class rdf:ID="DroughtCycleMitigation">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Extended drought cycles are especially problematic in arid climates.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="isBeneficialFor"/>
        </owl:onProperty>
        <owl:someValuesFrom>
          <owl:Class rdf:ID="AridRegionDwellers"/>
        </owl:someValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="ClimateStability"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="PermafrostMaintenance">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:ID="PolarDwellingPopulations"/>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#isBeneficialFor"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#ClimateStability"/>
    </rdfs:subClassOf>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Stability of permafrost provides structural support to buildings and infrastructure in polar regions.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="UnstableClimate">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Climate conditions that exist outside the comfortable range for human survival.  Classified as an endurant due to climate's long-term nature (vs. individual weather events, which would be perdurants).</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/es/eserv.owl#UndesiredEndurant"/>
  </owl:Class>
  <owl:Class rdf:about="#AllPeopleEverywhere">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Everyone's life to some degree gets affected by climate change</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/es/eserv.owl#HumanConsumer"/>
  </owl:Class>
  <owl:Class rdf:about="#ParticularlyVulnerableGroups">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Several groups of people are notably more susceptible to the ill effects of climate change</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/es/eserv.owl#HumanConsumer"/>
  </owl:Class>
  <owl:Class rdf:about="#PolarDwellingPopulations">
    <rdfs:subClassOf rdf:resource="#ParticularlyVulnerableGroups"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Facing permafrost melt - loss of roads, houses, Alaska Pipeline, etc.  Access to food sources - can't get places.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="GreenhouseGasEmissions">
    <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/es/eserv.owl#UndesiredEndurant"/>
  </owl:Class>
  <owl:Class rdf:ID="GoodConditionsForFoodFiberWater">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Poor subsistence populations directly collect water, food, fiber, and fuel from nature.  Deviations from optimal climate conditions may reduce the provisioning of these important goods.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:ID="PoorSubsistencePopulations"/>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#isBeneficialFor"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#ClimateStability"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#AridRegionDwellers">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Populations depending on potentially unreliable water supplies.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#ParticularlyVulnerableGroups"/>
  </owl:Class>
  <owl:Class rdf:ID="PopulationDensity">
    <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/es/eserv.owl#ContingentPolicyTarget"/>
  </owl:Class>
  <owl:Class rdf:ID="AvoidedIntenseStorms">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#ClimateStability"/>
    </rdfs:subClassOf>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >affect coastal populations - hurricanes, typhoons, etc.  More energy in atmosphere increases frequency and power of storms.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="CStorage">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom>
          <owl:Class rdf:about="#CSequesteringEcosystem"/>
        </owl:allValuesFrom>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="http://www.integratedmodelling.org/ks/es/eserv.owl#hasProducer"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/es/eserv.owl#Transformation"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Storage of carbon in terrestrial or oceanic ecosystems.  Is thhis NET - better way to express it since the process goes both ways?</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#PoorSubsistencePopulations">
    <rdfs:subClassOf rdf:resource="#ParticularlyVulnerableGroups"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >The poor who make their livelihood directly from subsistence living are likely to be hardest hit by climate change.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="VegetationCarbonStorage">
    <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/es/eserv.owl#EcosystemSupportedResource"/>
  </owl:Class>
  <owl:Class rdf:ID="Rainfall">
    <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/es/eserv.owl#EcosystemGeneratedResource"/>
  </owl:Class>
  <owl:Class rdf:about="#SafetyAndSecurity">
    <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/es/eserv.owl#HumanPolicyTarget"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >A stable climate reduces the likelihood of floods, storms, and other events that are potentially damaging to safety and security.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="FutureGenerations">
    <rdfs:subClassOf rdf:resource="#ParticularlyVulnerableGroups"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >The costs of climate change are overwhelmingly pushed onto future generations.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="SnowmeltDependentPopulations">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Populations dependent on snowpack for their water source - i.e., the American West or Asian nations dependent on Himalayan snowmelt.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#ParticularlyVulnerableGroups"/>
  </owl:Class>
  <owl:Class rdf:ID="CarbonStorage">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Storage (a stock) and sequestration (a flow) of carbon.  Both must be accounted for.  This service is for global carbon sequestration and storage, and does not include regional or local microcliamte regulation benefits, which would be classified elsewhere.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="http://www.integratedmodelling.org/ks/es/eserv.owl#humanFocus"/>
        </owl:onProperty>
        <owl:someValuesFrom>
          <owl:Class rdf:ID="Health"/>
        </owl:someValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="http://www.integratedmodelling.org/ks/es/eserv.owl#producesBenefit"/>
        </owl:onProperty>
        <owl:allValuesFrom>
          <owl:Class rdf:about="#ClimateStability"/>
        </owl:allValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="#HabitabilityOfLand"/>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="http://www.integratedmodelling.org/ks/es/eserv.owl#humanFocus"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#UnstableClimate"/>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="http://www.integratedmodelling.org/ks/es/eserv.owl#ecologicalFocus"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="http://www.integratedmodelling.org/ks/es/eserv.owl#humanFocus"/>
        </owl:onProperty>
        <owl:someValuesFrom rdf:resource="#SafetyAndSecurity"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="http://www.integratedmodelling.org/ks/es/eserv.owl#humanFocus"/>
        </owl:onProperty>
        <owl:someValuesFrom>
          <owl:Class rdf:ID="FoodFiberWater"/>
        </owl:someValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/es/eserv.owl#EcosystemService"/>
  </owl:Class>
  <owl:Class rdf:ID="CoastalProperty">
    <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/es/eserv.owl#ContingentPolicyTarget"/>
  </owl:Class>
  <owl:Class rdf:ID="SoilCarbonStorage">
    <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/es/eserv.owl#EcosystemSupportedResource"/>
  </owl:Class>
  <owl:Class rdf:ID="CStoringEcosystem">
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://www.integratedmodelling.org/ks/es/eserv.owl#EcosystemProducer"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Farmers">
    <rdfs:subClassOf rdf:resource="#ParticularlyVulnerableGroups"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Farmers benefit greatly from a stable climate.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#ClimateStability">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#ClimateExtremeMitigation"/>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="http://www.integratedmodelling.org/ks/es/eserv.owl#hasUsage"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="http://www.integratedmodelling.org/ks/es/eserv.owl#hasProvision"/>
        </owl:onProperty>
        <owl:someValuesFrom rdf:resource="#CUptake"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="http://www.integratedmodelling.org/ks/es/eserv.owl#hasProvision"/>
        </owl:onProperty>
        <owl:someValuesFrom rdf:resource="#CStorage"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/es/eserv.owl#EcosystemBenefit"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >A stable climate within comfortable range for survival of humans and the species (i.e., food and others) on which they depend.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="SuccessionalStage">
    <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/es/eserv.owl#EcosystemSupportedResource"/>
  </owl:Class>
  <owl:Class rdf:ID="SnowpackMaintenance">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="#SnowmeltDependentPopulations"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#isBeneficialFor"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="#ClimateStability"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >The maintenance of mountain glaciers and snowpack that deliver year-round water supplies to downstream populations.  Without snowpack, water supply becomes unreliable.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="SoilCNRatio">
    <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/es/eserv.owl#EcosystemSupportedResource"/>
  </owl:Class>
  <owl:Class rdf:about="#Health">
    <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/es/eserv.owl#HumanPolicyTarget"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >A stable climate will not promote the expansion of tropical diseases into formerly cooler regions.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="AvoidedSeaLevelRise">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Sea level rise threatens coastal populations and their built capital.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:ID="CoastalDwellers"/>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#isBeneficialFor"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="#ClimateStability"/>
  </owl:Class>
  <owl:Class rdf:about="#CSequesteringEcosystem">
    <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/es/eserv.owl#EcosystemProducer"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >An ecosystem capable of sequestering and storing carbon.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#CoastalDwellers">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Coastal dwellers are at risk of sea level rise and increased coastal storms</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#ParticularlyVulnerableGroups"/>
  </owl:Class>
  <owl:Class rdf:about="#FoodFiberWater">
    <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/es/eserv.owl#HumanPolicyTarget"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >A stable climate is more beneficial to agriculture, fisheries, and harvesting of natural materials that are typical to a given place.  A stable climate is also less likely to produce extreme rain or drought.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="CarbonSequestration">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#ClimateStability"/>
        <owl:onProperty rdf:resource="http://www.integratedmodelling.org/ks/es/eserv.owl#producesBenefit"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="http://www.integratedmodelling.org/ks/es/eserv.owl#humanFocus"/>
        </owl:onProperty>
        <owl:someValuesFrom rdf:resource="#SafetyAndSecurity"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="#Health"/>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="http://www.integratedmodelling.org/ks/es/eserv.owl#humanFocus"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#UnstableClimate"/>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="http://www.integratedmodelling.org/ks/es/eserv.owl#ecologicalFocus"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="http://www.integratedmodelling.org/ks/es/eserv.owl#humanFocus"/>
        </owl:onProperty>
        <owl:someValuesFrom rdf:resource="#HabitabilityOfLand"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="http://www.integratedmodelling.org/ks/es/eserv.owl#humanFocus"/>
        </owl:onProperty>
        <owl:someValuesFrom rdf:resource="#FoodFiberWater"/>
      </owl:Restriction>
    </rdfs:subClassOf>
