        <rdfs:comment rdf:datatype="&xsd;string"
            >Farmers benefit greatly from a stable climate.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonService.owl#FireFrequency -->

    <owl:Class rdf:about="#FireFrequency">
        <rdfs:subClassOf rdf:resource="&eserv;ContingentPolicyTarget"/>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/es/carbonService.owl#FireProneHousing -->

