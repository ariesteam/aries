  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:about="http://www.integratedmodelling.org/ks/es/eserv.owl#hasProvision">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </owl:FunctionalProperty>
