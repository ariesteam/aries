<?xml version="1.0" encoding="windows-1252"?>
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns="http://www.integratedmodelling.org/ks/es/esclass.owl#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:dc="http://purl.org/dc/elements/1.1/"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:eserv="http://www.integratedmodelling.org/ks/es/eserv.owl#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xml:base="http://www.integratedmodelling.org/ks/es/esclass.owl">
  <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="http://www.integratedmodelling.org/ks/es/eserv.owl"/>
    <rdfs:comment xml:lang="en">This ontology expands on the types of benefit, defining subclasses of benefit that may be useful to bridge to existing classification schemata for ecosystem services. The classes in the benefit ontology are all defined through restrictions, and are meant for reasoner-mediated classification of stated benefits. As such, there is no need to use the benefit ontology when defining services. </rdfs:comment>
  </owl:Ontology>
  <owl:Class rdf:ID="PreventionService">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:ID="AvoidanceBenefit"/>
        </owl:someValuesFrom>
        <owl:onProperty rdf:resource="http://www.integratedmodelling.org/ks/es/eserv.owl#producesBenefit"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:label>PreventionService</rdfs:label>
    <owl:versionInfo>Created by Ferdinando Villa on Tue Sep 11 15:33:04 EDT 2007</owl:versionInfo>
    <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/es/eserv.owl#EcosystemService"/>
  </owl:Class>
  <owl:Class rdf:about="#AvoidanceBenefit">
    <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/es/eserv.owl#EcosystemBenefit"/>
    <rdfs:label>AvoidanceBenefit</rdfs:label>
    <owl:versionInfo>Created by Ferdinando Villa on Tue Sep 11 15:33:49 EDT 2007</owl:versionInfo>
  </owl:Class>
  <owl:Class rdf:ID="TransformationBenefit">
    <rdfs:label>TransformationBenefit</rdfs:label>
    <owl:versionInfo>Created by Ferdinando Villa on Tue Sep 11 15:34:12 EDT 2007</owl:versionInfo>
    <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/es/eserv.owl#EcosystemBenefit"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://www.integratedmodelling.org/ks/es/eserv.owl#hasProvision"/>
        <owl:allValuesFrom rdf:resource="http://www.integratedmodelling.org/ks/es/eserv.owl#Transformation"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="ProductionBenefit">
    <rdfs:label>ProductionBenefit</rdfs:label>
    <owl:versionInfo>Created by Ferdinando Villa on Tue Sep 11 15:34:03 EDT 2007</owl:versionInfo>
    <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/es/eserv.owl#EcosystemBenefit"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://www.integratedmodelling.org/ks/es/eserv.owl#hasProvision"/>
        <owl:allValuesFrom rdf:resource="http://www.integratedmodelling.org/ks/es/eserv.owl#Production"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="ProvisionService">
    <rdfs:label>ProvisionService</rdfs:label>
    <owl:versionInfo>Created by Ferdinando Villa on Tue Sep 11 15:32:56 EDT 2007</owl:versionInfo>
    <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/es/eserv.owl#EcosystemService"/>
  </owl:Class>
  <owl:Class rdf:ID="ContemplationBenefit">
    <rdfs:label>ContemplationBenefit</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/es/eserv.owl#EcosystemBenefit"/>
    <owl:versionInfo>Created by Ferdinando Villa on Tue Sep 11 10:21:50 EDT 2007</owl:versionInfo>
  </owl:Class>
</rdf:RDF>
