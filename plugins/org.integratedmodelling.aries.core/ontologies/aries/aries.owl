<?xml version="1.0"?>
<rdf:RDF
    xmlns:obs="http://www.integratedmodelling.org/ks/science/observation.owl#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:protege="http://protege.stanford.edu/plugins/owl/protege#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns="http://www.integratedmodelling.org/ks/aries/aries.owl#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xml:base="http://www.integratedmodelling.org/ks/aries/aries.owl">
  <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="http://www.integratedmodelling.org/ks/science/observation.owl"/>
  </owl:Ontology>
  <owl:Class rdf:ID="Portfolio">
    <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/science/observation.owl#Identification"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >The observation of an ARIES case study. Is contingent on a number of specific benefit observations.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="BenefitObservation">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >The observation structure that contains all information about a specific 
    benefit. Depends on all models and data for the benefit.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/science/observation.owl#Identification"/>
  </owl:Class>
  <owl:Class rdf:ID="CaseStudy">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >The observable of an ARIES portfolio.</rdfs:comment>
  </owl:Class>
</rdf:RDF>

